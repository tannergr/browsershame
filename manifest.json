{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "options_page": "options.html",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [ "content.js"]
    }
  ],
  "browser_action": {
   "default_popup": "popup.html"
  }
}
